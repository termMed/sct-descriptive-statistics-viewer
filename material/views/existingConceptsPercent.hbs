<div class="col-md-12">
    <div class="card">
        <div class="card-height-indicator"></div>
        <div class="card-content">
            <div class="card-body">
                <h2>Changes on Existing Concepts - Percent</h2>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="bs-component">
                            <table class="table table-striped table-hover ">
                                <thead>
                                <tr>
                                    <th>Hierarchy</th>
                                    <th>Any change</th>
                                    <th>StatedDefChange</th>
                                    <th>InferredDefChange</th>
                                    <th>Primitive2SD</th>
                                    <th>SD2Primitive</th>
                                    <th>DescriptionChange</th>
                                    <th>FSNChange</th>
                                </tr>
                                </thead>
                                <tbody>
                                {{#each rows}}
                                    <tr class="small">
                                        <td>{{term}}</td>
                                        <td class="text-right">{{PercOfAnyChange}}%</td>
                                        <td class="text-right">{{PercOfStatedDefChange}}%</td>
                                        <td class="text-right">{{PercOfInferredDefChange}}%</td>
                                        <td class="text-right">{{PercOfPrimitive2SD}}%</td>
                                        <td class="text-right">{{PercOfSD2Primitive}}%</td>
                                        <td class="text-right">{{PercOfDescriptionChange}}%</td>
                                        <td class="text-right">{{PercOfFSNChange}}%</td>
                                    </tr>
                                {{/each}}
                                </tbody>
                            </table>
                        </div><!-- /example -->
                    </div>
                </div>
            </div>
            <footer class="card-footer">
                <!--<button class="btn btn-flat">Share</button>-->
                <!--<button class="btn btn-flat btn-warning">Learn More</button>-->
            </footer>
        </div>
    </div>
</div>
